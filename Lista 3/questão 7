#include <iostream>

int main() {
    int senhaDigitada;

    // Solicita ao usuário que insira a senha
    std::cout << "Digite a senha: ";
    std::cin >> senhaDigitada;

    // Verifica se a senha é válida (1234) e exibe a mensagem correspondente
    if (senhaDigitada == 1234) {
        std::cout << "ACESSO PERMITIDO\n";
    } else {
        std::cout << "ACESSO NEGADO\n";
    }

    return 0;
}
